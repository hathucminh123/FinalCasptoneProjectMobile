        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.trinhtam.JobSearchMobile"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "build": {
        "hermes": false
      }
    },
    "extra": {
      "eas": {
        "projectId": "3dcedb84-1cb6-444b-a923-f8feaf23a009"
      }
    },
    "owner": "hathucminh123"
